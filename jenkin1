pipeline {
    agent any
    stages {
        stage('git clone') {
            steps {
                checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[credentialsId: 'GIT_CRED', url: 'https://github.com/Cvaaaa/spring-petclinic-docker.git']])
                sh "${mavenCMD} clean install"
            }
        }
}