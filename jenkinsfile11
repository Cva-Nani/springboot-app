pipeline {
    agent any
    tools {
        maven 'maven'
    }
    

    stages {
        stage('GIt check') {
            steps {
                checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: 'GIT_CRED', url: 'https://github.com/Cvaaaa/springboot-app.git']])
            }
        }
       
        stage('mvn build') {
            steps {
                sh "mvn clean install package"
            }
        }
    }
}